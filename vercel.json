{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "src/server.ts": {
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.ts"
      }
    ],
    "devCommand": "npm run dev"
  }
  